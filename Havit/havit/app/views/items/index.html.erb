<p id="notice"><%= notice %></p>

<h1>Get</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Photo url</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

   

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= image_tag item.photo_url, width: 100 %></td>
        <td><%= item.title %></td>   
        <td><%= item.user.full_name if item.user %></td>
        <td><%= item.description %></td>
        <td><%= item.category %></td>
        <td><%= item.subcategory %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


